{
  "name": "afrittella/back-project",
  "description": "Just another admin panel for Laravel. Inspired by Backpack.",
  "require": {
    "php" : "~5.6|~7.0",
    "illuminate/support": "^5.4",
    "laravel/framework": "^5.4",
    "almasaeed2010/adminlte": "^2.3",
    "prologue/alerts": "0.4.*",
    "doctrine/dbal": "^2.5",
    "spatie/laravel-permission": "^1.7",
    "kalnoy/nestedset": "^4.2",
    "pendonl/laravel-fontawesome": "^1.0",
    "laravelcollective/html": "^5.4",
    "laravolt/avatar": "^1.8",
    "caouecs/laravel-lang": "~3.0",
    "intervention/image": "^2.3",
    "intervention/imagecache": "^2.3",
    "laravel/socialite": "^3.0",
    "illuminate/database": "^5.4"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Andrea Frittella",
      "email": "a.frittella@sintab.it",
      "role": "Chief Architect & Developer",
      "homepage": "http://www.andreafrittella.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Afrittella\\BackProject\\": "src/app"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7",
    "fzaninotto/faker": "~1.4",
    "orchestra/testbench": "^3.4",
    "orchestra/database": "*",
    "friendsofphp/php-cs-fixer": "^1.11"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Afrittella\\BackProject\\BackProjectServiceProvider"
      ],
    }
  }  
}
